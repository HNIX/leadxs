<div class="mb-6 bg-white dark:bg-gray-800 rounded-lg shadow overflow-hidden">
  <div class="p-4 border-b border-gray-200 dark:border-gray-700">
    <h3 class="text-lg font-medium">Bidding System Navigation</h3>
    <p class="text-sm text-gray-500 dark:text-gray-400">Manage and monitor your lead bidding system</p>
  </div>
  
  <% if @campaign.present? %>
  <div class="px-4 py-2 bg-blue-50 dark:bg-blue-900/20 border-b border-gray-200 dark:border-gray-700 flex justify-between items-center">
    <div class="flex items-center">
      <span class="text-sm font-medium text-blue-700 dark:text-blue-300">Filtered by Campaign:</span>
      <span class="text-sm ml-2 font-bold text-blue-800 dark:text-blue-200"><%= @campaign.name %></span>
    </div>
    <div>
      <% if controller_name == 'bid_dashboard' && action_name == 'index' %>
        <%= link_to bid_dashboard_path, class: "text-xs text-blue-600 hover:text-blue-800 dark:text-blue-400 dark:hover:text-blue-300" do %>
          <span>Clear Filter</span>
        <% end %>
      <% elsif controller_name == 'bid_dashboard' && action_name == 'real_time' %>
        <%= link_to real_time_bid_dashboard_path, class: "text-xs text-blue-600 hover:text-blue-800 dark:text-blue-400 dark:hover:text-blue-300" do %>
          <span>Clear Filter</span>
        <% end %>
      <% elsif controller_name == 'bid_reports' %>
        <%= link_to bid_reports_path, class: "text-xs text-blue-600 hover:text-blue-800 dark:text-blue-400 dark:hover:text-blue-300" do %>
          <span>Clear Filter</span>
        <% end %>
      <% end %>
    </div>
  </div>
  <% end %>
  
  <nav class="flex flex-col sm:flex-row">
    <%= link_to dashboard_bid_requests_path, 
        class: "px-4 py-3 flex items-center border-b sm:border-b-0 sm:border-r border-gray-200 dark:border-gray-700 #{current_page?(dashboard_bid_requests_path) ? 'bg-blue-50 dark:bg-blue-900/20 font-medium' : 'hover:bg-gray-50 dark:hover:bg-gray-700/30'}" do %>
      <div class="flex items-center">
        <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 mr-2 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 17h5l-1.405-1.405A2.032 2.032 0 0118 14.158V11a6.002 6.002 0 00-4-5.659V5a2 2 0 10-4 0v.341C7.67 6.165 6 8.388 6 11v3.159c0 .538-.214 1.055-.595 1.436L4 17h5m6 0v1a3 3 0 11-6 0v-1m6 0H9" />
        </svg>
        <div>
          <div>Bid Requests</div>
          <div class="text-xs text-gray-500 dark:text-gray-400">View & manage active bid requests</div>
        </div>
      </div>
    <% end %>
    
    <%= link_to bid_dashboard_path, 
        class: "px-4 py-3 flex items-center border-b sm:border-b-0 sm:border-r border-gray-200 dark:border-gray-700 #{current_page?(bid_dashboard_path) ? 'bg-blue-50 dark:bg-blue-900/20 font-medium' : 'hover:bg-gray-50 dark:hover:bg-gray-700/30'}" do %>
      <div class="flex items-center">
        <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 mr-2 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z" />
        </svg>
        <div>
          <div>Analytics Dashboard</div>
          <div class="text-xs text-gray-500 dark:text-gray-400">View historical bidding analytics</div>
        </div>
      </div>
    <% end %>
    
    <%= link_to real_time_bid_dashboard_path, 
        class: "px-4 py-3 flex items-center border-b sm:border-b-0 sm:border-r border-gray-200 dark:border-gray-700 #{current_page?(real_time_bid_dashboard_path) ? 'bg-blue-50 dark:bg-blue-900/20 font-medium' : 'hover:bg-gray-50 dark:hover:bg-gray-700/30'}" do %>
      <div class="flex items-center">
        <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 mr-2 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z" />
        </svg>
        <div>
          <div>Real-time Dashboard</div>
          <div class="text-xs text-gray-500 dark:text-gray-400">Live bidding activity and metrics</div>
        </div>
      </div>
    <% end %>
    
    <%= link_to bid_reports_path, 
        class: "px-4 py-3 flex items-center border-b sm:border-b-0 sm:border-r border-gray-200 dark:border-gray-700 #{controller_name == 'bid_reports' && action_name == 'index' ? 'bg-blue-50 dark:bg-blue-900/20 font-medium' : 'hover:bg-gray-50 dark:hover:bg-gray-700/30'}" do %>
      <div class="flex items-center">
        <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 mr-2 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 17v-2m3 2v-4m3 4v-6m2 10H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z" />
        </svg>
        <div>
          <div>Basic Reports</div>
          <div class="text-xs text-gray-500 dark:text-gray-400">Performance overview</div>
        </div>
      </div>
    <% end %>
    
    <%= link_to bid_economics_report_path, 
        class: "px-4 py-3 flex items-center border-b sm:border-b-0 sm:border-r border-gray-200 dark:border-gray-700 #{controller_name == 'bid_reports' && action_name == 'economics' ? 'bg-blue-50 dark:bg-blue-900/20 font-medium' : 'hover:bg-gray-50 dark:hover:bg-gray-700/30'}" do %>
      <div class="flex items-center">
        <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 mr-2 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 12l3-3 3 3 4-4M8 21l4-4 4 4M3 4h18M4 4h16v12a1 1 0 01-1 1H5a1 1 0 01-1-1V4z" />
        </svg>
        <div>
          <div>Bid Economics</div>
          <div class="text-xs text-gray-500 dark:text-gray-400">Bidding behavior analysis</div>
        </div>
      </div>
    <% end %>
    
    <%= link_to bid_revenue_report_path, 
        class: "px-4 py-3 flex items-center border-b sm:border-b-0 sm:border-r border-gray-200 dark:border-gray-700 #{controller_name == 'bid_reports' && action_name == 'revenue' ? 'bg-blue-50 dark:bg-blue-900/20 font-medium' : 'hover:bg-gray-50 dark:hover:bg-gray-700/30'}" do %>
      <div class="flex items-center">
        <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 mr-2 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
        </svg>
        <div>
          <div>Revenue Analysis</div>
          <div class="text-xs text-gray-500 dark:text-gray-400">Financial performance metrics</div>
        </div>
      </div>
    <% end %>
    
    <%= link_to bid_compliance_report_path, 
        class: "px-4 py-3 flex items-center border-b sm:border-b-0 sm:border-r border-gray-200 dark:border-gray-700 #{controller_name == 'bid_reports' && action_name == 'compliance' ? 'bg-blue-50 dark:bg-blue-900/20 font-medium' : 'hover:bg-gray-50 dark:hover:bg-gray-700/30'}" do %>
      <div class="flex items-center">
        <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 mr-2 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z" />
        </svg>
        <div>
          <div>Compliance Performance</div>
          <div class="text-xs text-gray-500 dark:text-gray-400">Verification & consent metrics</div>
        </div>
      </div>
    <% end %>
    
    <%= link_to bid_field_analysis_report_path, 
        class: "px-4 py-3 flex items-center border-b sm:border-b-0 sm:border-r border-gray-200 dark:border-gray-700 #{controller_name == 'bid_reports' && action_name == 'field_analysis' ? 'bg-blue-50 dark:bg-blue-900/20 font-medium' : 'hover:bg-gray-50 dark:hover:bg-gray-700/30'}" do %>
      <div class="flex items-center">
        <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 mr-2 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5H7a2 2 0 00-2 2v12a2 2 0 002 2h10a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2m-3 7h3m-3 4h3m-6-4h.01M9 16h.01" />
        </svg>
        <div>
          <div>Field-level Analysis</div>
          <div class="text-xs text-gray-500 dark:text-gray-400">Field impact on performance</div>
        </div>
      </div>
    <% end %>
    
    <%= link_to dashboard_anomaly_detections_path, 
        class: "px-4 py-3 flex items-center #{controller_name == 'anomaly_detections' ? 'bg-blue-50 dark:bg-blue-900/20 font-medium' : 'hover:bg-gray-50 dark:hover:bg-gray-700/30'}" do %>
      <div class="flex items-center">
        <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 mr-2 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 9v2m0 4h.01m-6.938 4h13.856c1.54 0 2.502-1.667 1.732-3L13.732 4c-.77-1.333-2.694-1.333-3.464 0L3.34 16c-.77 1.333.192 3 1.732 3z" />
        </svg>
        <div>
          <div>Anomaly Detection</div>
          <div class="text-xs text-gray-500 dark:text-gray-400">Monitor performance deviations</div>
        </div>
      </div>
    <% end %>
  </nav>
</div>